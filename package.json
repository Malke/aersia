{
  "name": "aersia",
  "friendlyname": "Aersia Player",
  "version": "0.1.7",
  "license": "MIT",
  "repository": "https://github.com/tprobinson/aersia.git",
  "esdoc": {
    "source": ".",
    "destination": "./docs",
    "unexportIdentifier": true,
    "undocumentIdentifier": true,
    "includes": [
      "^public/.+\\.jsx?$",
      "^src/services/.+\\.jsx?$",
      "^src/hooks/.+\\.jsx?$",
      "^src/[^/]+?.jsx?$"
    ],
    "experimentalProposal": {
      "classProperties": true,
      "objectRestSpread": true,
      "decorators": true,
      "doExpressions": true,
      "functionBind": true,
      "asyncGenerators": true,
      "exportExtensions": true,
      "dynamicImport": true
    },
    "plugins": []
  },
  "scripts": {
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint src/. test/. --config",
    "build": "webpack -p --colors",
    "mocha": "mocha test/ --recursive"
  },
  "dependencies": {
    "angular": "^1.5.9",
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "classnames": "^2.2.5",
    "clipboard": "^1.5.15",
    "desandro-classie": "^1.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "grunt": "^1.0.1",
    "grunt-autoprefixer": "^3.0.4",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^1.0.2",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-env": "^0.4.4",
    "grunt-html-validation": "^0.1.18",
    "grunt-image": "^2.2.2",
    "grunt-include-replace": "^5.0.0",
    "grunt-jsonmin": "^0.2.4",
    "grunt-modernizr": "^1.0.2",
    "grunt-processhtml": "^0.4.1",
    "grunt-real-favicon": "^0.2.0",
    "grunt-sass": "^1.2.1",
    "grunt-sass-globbing": "^1.5.1",
    "grunt-shell": "^2.1.0",
    "grunt-svgstore": "^1.0.0",
    "grunt-template": "^1.0.0",
    "grunt-uncss": "^0.6.1",
    "html-webpack-plugin": "^2.28.0",
    "import-glob": "^1.5.0",
    "ismobilejs": "^0.4.0",
    "jit-grunt": "^0.10.0",
    "js-cookie": "^2.1.3",
    "js-logger": "^1.3.0",
    "jsonlylightbox": "^0.5.3",
    "modernizr-webpack-plugin": "^1.0.5",
    "node-sass": "^3.13.0",
    "normalize.css": "^5.0.0",
    "object-fit-images": "^2.5.9",
    "perfect-scrollbar": "https://github.com/tprobinson/perfect-scrollbar.git",
    "ract": "^0.0.1",
    "react-dom": "^15.4.2",
    "seedrandom": "^2.4.2",
    "svg-sprite-loader": "^0.2.1",
    "time-grunt": "^1.4.0",
    "webpack": "^2.2.1",
    "x2js": "^3.0.1"
  },
  "devDependencies": {
    "esdoc": "^0.5.2",
    "eslint": "^3.15.0",
    "eslint-plugin-react": "^6.9.0",
    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-gh-pages": "^2.0.0",
    "mocha": "^3.2.0",
    "react-hot-loader": "^1.3.1",
    "watch-ignore-webpack-plugin": "^1.0.0",
    "webpack-dev-server": "^2.3.0",
    "webpack-hot-middleware": "^2.16.1"
  }
}
